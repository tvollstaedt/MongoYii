{
    "name": "pahanini/mongoyii",
    "description": "A Yii MongoDB ORM with lazy autoloading of subdocuments",
    "keywords": ["mongodb","orm","yii"],
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Pahanini",
            "homepage": "http://github.com/pahanini"
        }
    ],
    "autoload": {
    	"classmap": [
    		"./behaviors",
    		"./util",
    		"./validators",
			"./EMongoArrayModel.php",
    		"./EMongoClient.php",
    		"./EMongoCriteria.php",
    		"./EMongoCursor.php",
    		"./EMongoDataProvider.php",
    		"./EMongoDocument.php",
    		"./EMongoFile.php",
    		"./EMongoLogRoute.php",
    		"./EMongoModel.php",
    		"./EMongoSort.php"
    	]
    }
}